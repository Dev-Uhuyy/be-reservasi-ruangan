                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "email",
                                    "password",
                                    "role"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "John Doe"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "john@student.dinus.ac.id"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "password123"
                                    },
                                    "password_confirmation": {
                                        "type": "string",
                                        "example": "password123"
                                    },
                                    "role": {
                                        "type": "string",
                                        "enum": [
                                            "staff",
                                            "student"
                                        ],
                                        "example": "staff"
                                    },
                                    "nip": {
                                        "description": "Wajib untuk staff",
                                        "type": "string",
                                        "example": "STF12345"
                                    },
                                    "floor": {
                                        "description": "Wajib untuk staff",
                                        "type": "string",
                                        "example": "Lantai 3"
                                    },
                                    "nim": {
                                        "description": "Wajib untuk student",
                                        "type": "string",
                                        "example": "A11202113501"
                                    },
                                    "program": {
                                        "description": "Wajib untuk student",
                                        "type": "string",
                                        "example": "Informatika"
                                    }
                                },
                                "type": "object"
                            }
